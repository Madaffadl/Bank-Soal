<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".Kimia2">

    <LinearLayout
        android:id="@+id/linearLayout16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="46dp"
        android:layout_marginTop="46dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/soalk3"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="Fraksi mol etanol (Mr = 46) dalam air adalah 0,5. Jika dinyatakan dalam persen berat konsentrasi etanol adalah..."
            android:textColor="#000000"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="46dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup6">

        <TextView
            android:id="@+id/soalk4"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="Sebanyak 32 gram metanol (Mr = 32) dicampur dengan 72 gram air (Mr = 18). Jika tekanan uap air pada suhu tersebut 18 mmHg, maka tekanan uap larutan adalah  ??"
            android:textColor="#000000"
            android:textStyle="bold" />
    </LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="26dp"
        android:backgroundTint="#000000"
        android:text="Check"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup11"
        app:layout_constraintVertical_bias="1.0" />

    <RadioGroup
        android:id="@+id/radioGroup11"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout25">

        <RadioButton
            android:id="@+id/k4e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="12,6 mmHg" />

        <RadioButton
            android:id="@+id/k4a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14,4 mmHg"
            android:textColor="#FFEB3B" />

        <RadioButton
            android:id="@+id/k4d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14,1 mmHg" />

        <RadioButton
            android:id="@+id/k4b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15,3 mmHg" />
    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup6"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

        <RadioButton
            android:id="@+id/k3d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="80 %" />

        <RadioButton
            android:id="@+id/k3b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="66 %" />

        <RadioButton
            android:id="@+id/k3a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="72 %"
            android:textColor="#FFEB3B" />

        <RadioButton
            android:id="@+id/k3e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="76 %" />
    </RadioGroup>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginBottom="26dp"
        android:backgroundTint="@color/black"
        android:onClick="lembaark2back"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup11"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>